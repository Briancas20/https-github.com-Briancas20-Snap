    <!-- Modal Import Structure -->

    <div id="modal-for-import" class="modal modal-fixed-footer" style="width:466px;">

        <div class="modal-content" style="padding:0;">

            <div class="blue darken-4" style="padding:24px 0 24px 24px;">

